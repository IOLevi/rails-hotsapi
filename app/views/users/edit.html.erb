<h1>Edit</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(current_user) do |f| %>
      <%= render 'error_messages' %>

      <%= f.label :hotslogs %>
      <%= f.text_field :hotslogs, class: 'form-control' %>

      <%= f.submit "Save Change", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>